DataItemList.cs

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading;
using System.Threading.Tasks;

namespace CourseProdject
{
    public class DataItemList
    {
        public List<DataItem> Items { get; set; }

        public DataItemList(List<DataItem> items) 
        {
            Items = items;
        }

        public void ClearList()
        {
            Items.Clear();
        }

        public int[] CountFemaleAndMalePerson()
        {
            int[] counts = new int[2];
            foreach (var item in Items)
            {
                if(item.Sex == "ЧОЛОВІЧА")
                {
                    counts[0]++;
                    continue;
                }

                if(item.Sex == "ЖІНОЧА")
                {
                    counts[1]++;
                    continue;
                }
            }

            return counts;
        }
    }
}

DataItem.cs

using System;
using System.Collections.Generic;
using System.Linq;
using System.Net.Http.Headers;
using System.Text;
using System.Threading.Tasks;
using static System.Windows.Forms.VisualStyles.VisualStyleElement.ListView;
using System.Xml.Linq;

namespace CourseProdject
{
    [Serializable]
    public class DataItem
    {
        public string Id { get; set; }
        public string Ovd { get; set; }
        public string Category { get; set; }
        public string FirstNameU { get; set; }
        public string LastNameU { get; set; }
        public string MiddleNameU { get; set; }
        public string FirstNameR { get; set; }
        public string LastNameR { get; set; }
        public string MiddleNameR { get; set; }
        public string FirstNameE { get; set; }
        public string LastNameE { get; set; }
        public string MiddleNameE { get; set; }
        public string BirthDate { get; set; }
        public string Sex { get; set; }
        public string LostDate { get; set; }
        public string LostPlace { get; set; }
        public string ArticleCrim { get; set; }
        public string Restraint { get; set; }
        public string Contact { get; set; }

        public DataItem(string id, string ovd, string category, string firstNameU, 
            string lastNameU, string middleNameU, string firstNameR, string lastNameR, string middleNameR, 
            string firstNameE, string lastNameE, string middleNameE, string birthDate, string sex, string lostDate,
            string lostPlace, string articleCrim, string restraint, string contact)
        {
            Id = id;
            Ovd = ovd;
            Category = category;
            FirstNameU = firstNameU;
            LastNameU = lastNameU;
            MiddleNameU = middleNameU;
            FirstNameR = firstNameR;
            LastNameR = lastNameR;
            MiddleNameR = middleNameR;
            FirstNameE = firstNameE;
            LastNameE = lastNameE;
            MiddleNameE = middleNameE;
            BirthDate = birthDate;
            Sex = sex;
            LostDate = lostDate;
            LostPlace = lostPlace;
            ArticleCrim = articleCrim;
            Restraint = restraint;
            Contact = contact;
        }
        public override string ToString()
        {
            return $"{Id.PadRight(16)}\t{Ovd.PadRight(150)}\t{Category.PadRight(200)}" +
                $"\t{FirstNameU.PadRight(20)}\t{LastNameU.PadRight(20)}\t{MiddleNameU.PadRight(20)}" +
                $"\t{FirstNameR.PadRight(20)}\t{LastNameR.PadRight(20)}\t{MiddleNameR.PadRight(20)}" +
                $"\t{FirstNameE.PadRight(20)}\t{LastNameE.PadRight(20)}\t{MiddleNameE.PadRight(20)}" +
                $"\t{BirthDate.PadRight(12)}\t{Sex.PadRight(10)}\t{LostDate.PadRight(12)}\t{LostPlace.PadRight(100)}" +
                $"\t{ArticleCrim.PadRight(20)}\t{Restraint.PadRight(100)}\t{Contact.PadRight(100)}";
        }
    }
}

Program.cs

using System;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace CourseProdject
{
    internal static class Program
    {
        /// <summary>
        /// The main entry point for the application.
        /// </summary>
        [STAThread]
        static void Main()
        {
            Application.EnableVisualStyles();
            Application.SetCompatibleTextRenderingDefault(false);
            Application.Run(new MainForm());
        }
    }
}

